<div class="d-flex justify-content-center">
    <div class="row">
        <div class="col">
            <table class="table table-striped table-borderless">
                <thead>
                    <tr>
                        <th>Document Name</th>
                        <th>Municipality</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var searchItem in SearchResultList)
                    {
                        <tr>
                            <td>
                                @searchItem.DocumentName
                            </td>
                            <td>
                                @searchItem.Municipality
                            </td>
                            <td>
                                <a href="@searchItem.DocumentLink">Download</a>
                            </td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>

    </div>
</div>
